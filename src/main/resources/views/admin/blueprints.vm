#define($body-html)
<div class="container-fluid">
    <table class="table">
        <thead>
        <tr>
            <th>
                $blueprintName
            </th>
            <th>
                $blueprintPageTemplate
            </th>
            <th>
                $blueprintPageBlock
            </th>
            <th>
                $blueprintAddableBlock
            </th>
            <th>
                $blueprintProperties
            </th>
            <th>
                $blueprintScripts
            </th>
            <th>
                $blueprintLinks
            </th>
            <th>
                $blueprintLanguages
            </th>
            <th>
                $blueprintId
            </th>
            <th>
                $blueprintApplicationVersion
            </th>
            <th>
                $blueprintCreatedBy
            </th>
            <th>
                $blueprintCreatedAt
            </th>
            <th>
                $blueprintUpdatedBy
            </th>
            <th>
                $blueprintUpdatedAt
            </th>
            <th>
                $blueprintDeleted
            </th>
        </tr>
        </thead>
        <tbody>
            #foreach($blueprint in $blueprints)
            <tr>
                <td>
                    $!blueprint.getName()
                </td>
                <td>
                    $!blueprint.getPageTemplateName()
                </td>
                <td>
                    $!blueprint.isPageBlock()
                </td>
                <td>
                    $!blueprint.isAddableBlock()
                </td>
                <td>
                    $!blueprint.getProperties().size()
                </td>
                <td>
                    $!blueprint.getScripts().size()
                </td>
                <td>
                    $!blueprint.getLinks().size()
                </td>
                <td>
                    $!blueprint.getLanguages()
                </td>
                <td>
                    $!blueprint.getId().getUnversionedId()
                </td>
                <td>
                    $!blueprint.getApplicationVersion()
                </td>
                <td>
                    $!blueprint.getCreatedBy()
                </td>
                <td>
                    $!blueprint.getCreatedAt()
                </td>
                <td>
                    $!blueprint.getUpdatedBy()
                </td>
                <td>
                    $!blueprint.getUpdatedAt()
                </td>
                <td>
                    $!blueprint.getDeleted()
                </td>
            </tr>
            #end
        </tbody>
    </table>
</div>
#end

#parse("/views/admin/main.html")